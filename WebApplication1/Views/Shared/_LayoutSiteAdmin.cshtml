    <script>
        var Email = '';
        $(document).ready(function () {
            handleOtpInput();
            cartInitialize();
            $('#saveLgnreq').click(() => {
                Email = $('#emiallp').val();
                let obj = {
                    EmailOrMobile: Email,
                    OTP: $('#lgnOTP').val()
                }
                $.post('/Account/LoginViaOTP', obj).done((res) => {
                    if (res.statusCode == 1) {
                        $('.emails').addClass('d-none')
                        $('#saveLgnreq').addClass('d-none')
                        document.getElementById('otpSection').style.display = 'block';
                        $('label[for="otpInput"]').text('Enter the 6-digit OTP We have sent a verification code to ' + obj.EmailOrMobile);
                        const otpInputs = document.querySelectorAll('.otp-box');
                        otpInputs.forEach((input, index) => {
                            input.addEventListener('input', () => {
                                if (input.value.length === 1 && index < otpInputs.length - 1) {
                                    otpInputs[index + 1].focus();
                                }
                            });
                        });

                    }
                })
            });
            $("#addNewAddress").click((res) => {
                $("#newaddressModal").html(res);
                $("#").modal('show');
            });
        });

        function OpenCart() {
            $.post('/Website/Cart').done((result) => {
                $('.cart-modal').html(result);
                $('#myModal').show();
            })
        }

        function handleOtpInput() {
            const otpInputs = $('.otp-box');
            otpInputs.on('input', function () {
                let otpValue = '';
                let allFilled = true;
                otpInputs.each(function () {
                    if ($(this).val().length === 0) {
                        allFilled = false;
                    }
                    otpValue += $(this).val();
                });
                if (allFilled && otpValue.length === 6) {
                    matchOtp(otpValue);
                }
            });
        }
        function matchOtp(otp) {
            let obj = {
                EmailOrMobile: Email,
                OTP: otp
            }
            $.post('/Account/LoginViaOTP', obj).done((res) => {
                if (res.statusCode == 1) {
                    let lgnsucss = `
                            <div class="logo text-center">
                                <a href="index.html">
                                    <img src="https://upload.wikimedia.org/wikipedia/commons/3/3b/Eo_circle_green_checkmark.svg" alt="logo" style="width: 50px;">
                                </a>
                            </div>
                            <h5 class="text-center" id="lgnSuccess">Login Successfull</h5>`;
                    $('#logintopoceedModal .modal-footer').addClass('d-none');
                    $('#logintopoceedModal .modal-body').html(lgnsucss);
                    setTimeout(() => {
                        location.reload();
                    }, 3000);
                } else {
                    QAlert(-1, 'User does not Exists');
                }

            });

        }

